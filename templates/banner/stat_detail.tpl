<table border="0" cellspacing="1" cellpadding="2" bgcolor="#CCCCCC">
<tr>
	<td class="fcaption2" colspan=3>{VAR:image}</td>
</tr>
<tr>
	<td colspan=3 class="fcaption2">{VAR:LC_BANNER_LOOKS}:</td>
</tr>
<!-- SUB: VIEWS_LINE -->
<tr>
	<td class="fcaption2" valign="top">{VAR:date}</td>
	<td class="fcaption2" valign="center"><img src='{VAR:baseurl}/images/bar.gif' height=5 width='{VAR:vlen}'></td>
	<td class="fform">{VAR:views}</td>
</tr>
<!-- END SUB: VIEWS_LINE -->
<tr>
	<td colspan=2 class="fcaption2">{VAR:LC_BANNER_TOG_LOOKS_WITHIN_7_DAYS}:</td>
