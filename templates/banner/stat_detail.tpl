	<td class="fcaption2" valign="top">{VAR:date}</td>
	<td class="fcaption2" valign="center"><img src='{VAR:baseurl}/images/bar.gif' height=5 width='{VAR:clen}'></td>
	<td class="fform">{VAR:clics}</td>
</tr>
<!-- END SUB: CLICKS_LINE -->
<tr>
	<td colspan=2 class="fcaption2">{VAR:LC_BANNER_TOG_CKLICKS_WITHIN_7DAYS}:</td>
	<td class="fform">{VAR:t_clics}</td>
</tr>
