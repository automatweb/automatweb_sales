<table border="0" cellspacing="3">
<!-- SUB: ROW -->
<tr>
	<!-- SUB: COL -->
	<td colspan="{VAR:colspan}" align="{VAR:align}">
		<table border="0">
			<tr>
				<td align="right">
					<!-- SUB: UP_ARROW_LEFT -->
					<img src="{VAR:baseurl}/automatweb/images/wf/arrow_up_left.png" alt="{VAR:alt}" title="{VAR:alt}">
					<!-- END SUB: UP_ARROW_LEFT -->
				&nbsp;</td>
				<td align="center">
					<!-- SUB: UP_ARROW -->
					<img src="{VAR:baseurl}/automatweb/images/wf/arrow_up.png" alt="{VAR:alt}" title="{VAR:alt}">
					<!-- END SUB: UP_ARROW -->
				&nbsp;</td>
				<td align="left">
					<!-- SUB: UP_ARROW_RIGHT -->
					<img src="{VAR:baseurl}/automatweb/images/wf/arrow_up_right.png" alt="{VAR:alt}" title="{VAR:alt}">
					<!-- END SUB: UP_ARROW_RIGHT -->
				&nbsp;</td>
			</tr>
			<tr>
				<td align="right">
					<!-- SUB: LEFT_ARROW -->
					<img src="{VAR:baseurl}/automatweb/images/wf/arrow_left.png" alt="{VAR:alt}" title="{VAR:alt}">
					<!-- END SUB: LEFT_ARROW -->
				&nbsp;</td>
				<td align="center">{VAR:name}</td>
				<td align="left">
					<!-- SUB: RIGHT_ARROW -->
					<img src="{VAR:baseurl}/automatweb/images/wf/arrow_right.png" alt="{VAR:alt}" title="{VAR:alt}">
					<!-- END SUB: LEFT_ARROW -->
				&nbsp;</td>
			</tr>
			<tr>
				<td align="right">
					<!-- SUB: DOWN_ARROW_LEFT -->
					<img src="{VAR:baseurl}/automatweb/images/wf/arrow_down_left.png" alt="{VAR:alt}" title="{VAR:alt}">
					<!-- END SUB: DOWN_ARROW_LEFT -->
				&nbsp;</td>
				<td align="center">
					<!-- SUB: DOWN_ARROW -->
					<img src="{VAR:baseurl}/automatweb/images/wf/arrow_down.png" alt="{VAR:alt}" title="{VAR:alt}">
					<!-- END SUB: DOWN_ARROW -->
				&nbsp;</td>
				<td align="left">
					<!-- SUB: DOWN_ARROW_RIGHT -->
					<img src="{VAR:baseurl}/automatweb/images/wf/arrow_down_right.png" alt="{VAR:alt}" title="{VAR:alt}">
					<!-- END SUB: DOWN_ARROW_RIGHT -->
				&nbsp;</td>
			</tr>
		</table>
	</td>
	<!-- END SUB: COL -->

	<!-- SUB: COL_EMPTY -->
	<td>&nbsp;</td>
	<!-- END SUB: COL_EMPTY -->
</tr>
<!-- END SUB: ROW -->
</table>
