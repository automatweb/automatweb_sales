			<function name="manage">
				<arguments>
					<required name="doc" />
					<optional name="link_url" />
				</arguments>
			</function>
		</action>

		<action name="topf">
			<function name="topf">
				<arguments>
					<required name="doc" />
				</arguments>
			</function>
		</action>

		<action name="manager">
			<function name="manager">
				<arguments>
					<required name="docid" />
				</arguments>
			</function>
		</action>

	</class>
</orb>
