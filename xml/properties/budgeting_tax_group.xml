<?xml version='1.0'?>
<properties>
      <property>
            <name>tax_tree</name>
            <table>objects</table>
            <group>flow</group>
            <type>treeview</type>
            <parent>tax_tree</parent>
            <store>no</store>
            <no_caption>1</no_caption>
            <field>tax_tree</field>
      </property>
      <property>
            <name>tax_s_from_acct</name>
            <table>objects</table>
            <group>flow</group>
            <type>textbox</type>
            <parent>tax_search</parent>
            <store>no</store>
            <captionside>top</captionside>
            <size>20</size>
            <field>tax_s_from_acct</field>
            <caption>Kontolt</caption>
      </property>
      <property>
            <name>tax_s_to_acct</name>
            <table>objects</table>
            <group>flow</group>
            <type>textbox</type>
            <parent>tax_search</parent>
            <store>no</store>
            <captionside>top</captionside>
            <size>20</size>
            <field>tax_s_to_acct</field>
            <caption>Kontole</caption>
      </property>
      <property>
            <name>tax_s_date_from</name>
            <table>objects</table>
            <group>flow</group>
            <type>date_select</type>
            <format>
                  <num_0>day_textbox</num_0>
                  <num_1>month_textbox</num_1>
                  <num_2>year_textbox</num_2>
            </format>
            <parent>tax_search</parent>
            <store>no</store>
            <captionside>top</captionside>
            <default>-1</default>
            <field>tax_s_date_from</field>
            <caption>Alates</caption>
      </property>
      <property>
            <name>tax_s_date_to</name>
            <table>objects</table>
            <group>flow</group>
            <type>date_select</type>
            <format>
                  <num_0>day_textbox</num_0>
                  <num_1>month_textbox</num_1>
                  <num_2>year_textbox</num_2>
            </format>
            <parent>tax_search</parent>
            <store>no</store>
            <captionside>top</captionside>
            <default>-1</default>
            <field>tax_s_date_to</field>
            <caption>Kuni</caption>
      </property>
      <property>
            <name>tax_s_sbt</name>
            <table>objects</table>
            <group>flow</group>
            <type>submit</type>
            <parent>tax_search</parent>
            <store>no</store>
            <no_caption>1</no_caption>
            <field>tax_s_sbt</field>
            <caption>Otsi</caption>
      </property>
      <property>
            <name>flow_table</name>
            <table>objects</table>
            <group>flow</group>
            <type>table</type>
            <parent>tax_table</parent>
            <store>no</store>
            <no_caption>1</no_caption>
            <field>flow_table</field>
      </property>
      <classinfo>
            <syslog_type>ST_BUDGETING_TAX_GROUP</syslog_type>
            <relationmgr>yes</relationmgr>
            <no_status>1</no_status>
            <prop_cb>1</prop_cb>
            <maintainer>kristo</maintainer>
      </classinfo>
      <groupinfo>
            <flow>
                  <caption>&amp;amp;Uuml;lekanded</caption>
                  <submit>no</submit>
                  <save>no</save>
            </flow>
      </groupinfo>
      <layout>
            <flow_split>
                  <type>hbox</type>
                  <width>20%:80%</width>
                  <group>flow</group>
                  <area_caption></area_caption>
            </flow_split>
            <flow_left>
                  <type>vbox</type>
                  <parent>flow_split</parent>
                  <group>flow</group>
                  <area_caption></area_caption>
            </flow_left>
            <tax_tree>
                  <parent>flow_left</parent>
                  <type>vbox</type>
                  <closeable>1</closeable>
                  <area_caption>Maksude&amp;amp;nbsp;puu</area_caption>
                  <group>flow</group>
            </tax_tree>
            <tax_search>
                  <parent>flow_left</parent>
                  <type>vbox</type>
                  <closeable>1</closeable>
                  <area_caption>Otsing</area_caption>
                  <group>flow</group>
            </tax_search>
            <tax_table>
                  <parent>flow_split</parent>
                  <type>vbox</type>
                  <group>flow</group>
                  <area_caption></area_caption>
            </tax_table>
      </layout>
</properties>

