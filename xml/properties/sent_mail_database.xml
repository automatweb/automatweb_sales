<?xml version='1.0'?>
<properties>
      <property>
            <name>ml_tb</name>
            <table>objects</table>
            <group>mails</group>
            <type>toolbar</type>
            <no_caption>1</no_caption>
            <store>no</store>
            <field>ml_tb</field>
      </property>
      <property>
            <name>date_from</name>
            <table>objects</table>
            <group>mails</group>
            <type>date_select</type>
            <captionside>top</captionside>
            <store>no</store>
            <parent>mail_left_pane</parent>
            <field>date_from</field>
            <caption>Alates</caption>
      </property>
      <property>
            <name>date_to</name>
            <table>objects</table>
            <group>mails</group>
            <type>date_select</type>
            <captionside>top</captionside>
            <store>no</store>
            <parent>mail_left_pane</parent>
            <field>date_to</field>
            <caption>Kuni</caption>
      </property>
      <property>
            <name>s_sbt</name>
            <table>objects</table>
            <group>mails</group>
            <type>submit</type>
            <no_caption>1</no_caption>
            <store>no</store>
            <parent>mail_left_pane</parent>
            <field>s_sbt</field>
            <caption>Otsi</caption>
      </property>
      <property>
            <name>mail_table</name>
            <table>objects</table>
            <group>mails</group>
            <type>table</type>
            <store>no</store>
            <no_caption>1</no_caption>
            <parent>mail_view</parent>
            <field>mail_table</field>
      </property>
      <classinfo>
            <syslog_type>ST_SENT_MAIL_DATABASE</syslog_type>
            <relationmgr>yes</relationmgr>
            <no_comment>1</no_comment>
            <no_status>1</no_status>
            <prop_cb>1</prop_cb>
      </classinfo>
      <groupinfo>
            <mails>
                  <caption>Meilid</caption>
            </mails>
      </groupinfo>
      <layout>
            <mail_view>
                  <type>hbox</type>
                  <width>30%:80%</width>
                  <group>mails</group>
                  <area_caption></area_caption>
            </mail_view>
            <mail_left_pane>
                  <type>vbox</type>
                  <closeable>1</closeable>
                  <area_caption>Otsing</area_caption>
                  <parent>mail_view</parent>
                  <group>mails</group>
            </mail_left_pane>
      </layout>
</properties>

